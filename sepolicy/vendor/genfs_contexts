# Battery
genfscon sysfs /class/qcom-battery                                                  u:object_r:vendor_sysfs_battery_supply:s0

# Display
genfscon sysfs /devices/platform/soc/soc:qcom,dsi-display-primary/dimlayer_exposure u:object_r:vendor_sysfs_graphics:s0

# Fingerprint
genfscon sysfs /devices/platform/soc/soc:xiaomi_fingerprint/fingerdown              u:object_r:vendor_sysfs_fingerprint:s0

# Sensors
genfscon sysfs /devices/virtual/touch/touch_dev/ear_sensor                          u:object_r:vendor_sysfs_prox:s0
genfscon sysfs /devices/virtual/touch/touch_dev/palm_sensor                         u:object_r:vendor_sysfs_prox:s0
genfscon sysfs /devices/virtual/touch/touch_dev/ear_sensor_data                     u:object_r:vendor_sysfs_prox:s0

